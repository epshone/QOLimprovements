{
  "name": "appianTabs",
  "version": "0.1",
  "browser_action":
  {
    "default_icon": "assets/logo.png",
    "default_popup": "popup.html"
  },
  "background":
  {
    "scripts": ["js/request.js", "js/background.js"]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "commands": {
    "open-db": {
      "suggested_key":{
        "default": "Alt+1"
      },
      "description": "Open DB"
    },
    "open-designer": {
      "suggested_key":{
        "default": "Alt+2"
      },
      "description": "Open Designer"
    },
    "open-design": {
      "suggested_key":{
        "default": "Alt+3"
      },
      "description": "Open Design"
    },
    "open-design-rule": {
      "suggested_key":{
        "default": "Alt+4"
      },
      "description": "Open Design Rule"
    },
    "open-tempo": {
      "description": "Open Tempo"
    },
    "open-admin": {
      "description": "Open Admin"
    },
    "open-interface": {
      "description": "Open Interface"
    }
  },
  "manifest_version": 2
}
